<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <appSettings>
        <!--Профиль со списком игр Default/Dropmania/RGG. Можно добавлять свои папки со своими списками-->
        <add key="Profile" value="RGG" />        
        <!--Размеры окна при запуске рандомайзера-->
        <add key="WindowWidth" value="1000" />
        <add key="WindowHeight" value="800" />         
        <!--Файл картинки с бэкграундом (Размер по умолчанию 984x690?). Можно добавлять свои картинки-->
        <add key="Background" value="background.png" />
        <!--Папка с музыкой. Можно добавлять свои папки с музыкой-->
        <add key="SoundSystem" value="Retro" />
        <!--Название шрифта-->
        <add key="FontName" value="Segoe UI" />
        <!--Размер шрифта (Big - центральная строчка; Medium - 2,4 строчки; Small - 1,5 строчки )-->
        <add key="FontBigSize" value="48" />
        <add key="FontMediumSize" value="36" />
        <add key="FontSmallSize" value="24" />
        <!--Цвет текста игры-->
        <add key="GameRollColor" value="#FFFFFF" />
        <!--Цвет текста выделенной в центре игры-->
        <add key="SelectedGameRollColor" value="#BE9A21" />
        <!--Цвет текста предмета (это для дропмании/rhg)-->
        <add key="SelectedItemColor" value="#0bb3d9" />
    </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Ninject" publicKeyToken="c7192dc5380945e7" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.3.4.0" newVersion="3.3.4.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>